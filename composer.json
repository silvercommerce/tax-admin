{
    "name": "silvercommerce/tax-admin",
    "description": "Module allowing creation of tax rates and categories in the CMS via SiteConfig",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/silvercommerce/tax-admin",
    "keywords": [
        "silverstripe",
        "commerce",
        "ecommerce",
        "tax"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Morven Lewis-Everley",
            "email": "morven@ilateral.co.uk"
        },
        {
            "name": "Mark Anderson",
            "email": "mark@ilateral.co.uk"
        }
    ],
    "support": {
        "issues": "https://github.com/silvercommerce/tax-admin/issues"
    },
    "require": {
        "silverstripe/framework": "~4.11 || ^5",
        "silverstripe/siteconfig": "~4.11 || ^5",
        "silvercommerce/geozones": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SilverCommerce\\TaxAdmin\\": "src/",
            "SilverCommerce\\TaxAdmin\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "vendor/bin/phpcs src/ tests/",
        "lint-clean": "vendor/bin/phpcbf src/ tests/"
    },
    "suggest": {
        "silverstripe/cms": "Add full content management support to your site"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:catharsisjelly/silverstripe-gridfieldextensions.git"
        }
    ],
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
